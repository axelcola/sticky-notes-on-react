((task) => !task.deleted);\r\n      setDelTasks(newArrayNotes);\r\n      setDelTasks([]);\r\n      window.localStorage.setItem(\"notes\", JSON.stringify(newArrayNotes));\r\n      console.log(delTasks);\r\n    }\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <NavbarNotes\r\n        searcher={searcherInfo}\r\n        number={delTasks}\r\n        paperbin={true}\r\n        deleteAll={deleteAllNotes}\r\n      />\r\n      <div className=\"container\">\r\n        <div className=\" notelist\">\r\n          {delTasks.map((task) => (\r\n            <Note\r\n              key={task.id}\r\n              id={task.id}\r\n              text={task.text}\r\n              deleted={task.deleted}\r\n              deleteNote={restoreNote}\r\n              className={task.deleted ? \"note\" : \"note deleted\"}\r\n              deletePerm={deletePerm}\r\n              date={task.date}\r\n              color={task.color}\r\n              title={task.title}\r\n            />\r\n          ))}\r\n        </div>\r\n      </div>\r\n      {delTasks.length ? (\r\n        <></>\r\n      ) : (\r\n        <>\r\n          <div className=\"title-container\">\r\n            <h4>Deleted notes will appear here.</h4>\r\n          </div>\r\n        </>\r\n      )}\r\n    </>\r\n  );\r\n};\r\n\r\nexport default PaperBin;\r\n","E:\\proyectos-web\\react-sticky-notes\\src\\components\\getDate.js",[],[],"E:\\proyectos-web\\react-sticky-notes\\src\\components\\ColorDropdown.js",[],[],"E:\\proyectos-web\\react-sticky-notes\\src\\components\\Color.js",[],[],"E:\\proyectos-web\\react-sticky-notes\\src\\components\\Footer.js",[],[],{"ruleId":"64","severity":1,"message":"65","line":4,"column":10,"nodeType":"66","messageId":"67","endLine":4,"endColumn":17},"no-unused-vars","'BiTrash' is defined but never used.","Identifier","unusedVar"]